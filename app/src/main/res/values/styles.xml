<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme0" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorOnPrimary">@color/black</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
    </style>

    <style name="Theme1" parent="Theme0">
        <item name="colorPrimary">@color/deep_orange_100</item>
        <item name="colorPrimaryDark">@color/deep_orange_100_dark</item>
        <item name="colorPrimaryVariant">@color/deep_orange_100_dark</item>
        <item name="colorSecondary">@color/deep_orange_100</item>
        <item name="colorSecondaryVariant">@color/deep_orange_100_light</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/deep_orange_100_dark</item>
    </style>

    <style name="Theme2" parent="Theme0">
        <item name="colorPrimary">@color/yellow_100</item>
        <item name="colorPrimaryDark">@color/yellow_100_dark</item>
        <item name="colorPrimaryVariant">@color/yellow_100_dark</item>
        <item name="colorSecondary">@color/yellow_100</item>
        <item name="colorSecondaryVariant">@color/yellow_100_light</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/yellow_100_dark</item>
    </style>

    <style name="Theme3" parent="Theme0">
        <item name="colorPrimary">@color/light_green_100</item>
        <item name="colorPrimaryDark">@color/light_green_100_dark</item>
        <item name="colorPrimaryVariant">@color/light_green_100_dark</item>
        <item name="colorSecondary">@color/light_green_100</item>
        <item name="colorSecondaryVariant">@color/light_green_100_light</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/light_green_100_dark</item>
    </style>

    <style name="Theme4" parent="Theme0">
        <item name="colorPrimary">@color/cyan_100</item>
        <item name="colorPrimaryDark">@color/cyan_100_dark</item>
        <item name="colorPrimaryVariant">@color/cyan_100_dark</item>
        <item name="colorSecondary">@color/cyan_100</item>
        <item name="colorSecondaryVariant">@color/cyan_100_light</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/cyan_100_dark</item>
    </style>

    <style name="Theme5" parent="Theme0">
        <item name="colorPrimary">@color/deep_purple_100</item>
        <item name="colorPrimaryDark">@color/deep_purple_100_dark</item>
        <item name="colorPrimaryVariant">@color/deep_purple_100_dark</item>
        <item name="colorSecondary">@color/deep_purple_100</item>
        <item name="colorSecondaryVariant">@color/deep_purple_100_light</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/deep_purple_100_dark</item>
    </style>
</resources>
